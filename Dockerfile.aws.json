{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "fc-api-ios",
      "host": {
        "sourcePath": "/var/app/current/django_app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "fc-api-ios",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4567
        }
      ],
    }
  ]
}